#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int N, isPrime = 1;
    scanf("%d", &N);  // Read the input number

    if (N <= 1) {
        isPrime = 0;  // Numbers <= 1 are not prime
    } else {
        for (int i = 2; i <= sqrt(N); i++) {
            if (N % i == 0) {
                isPrime = 0;  // Found a divisor, not prime
                break;
            }
        }
    }

    if (isPrime)
        printf("Prime\n");
    else
        printf("Not Prime\n");
    return 0;
}
