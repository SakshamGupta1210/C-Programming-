#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   int n, i, a=0, b=1, sum;
    
    scanf("%d", &n);
    
    for ( i = 0; i < n; i++){
        if(i==0){
            printf("%d ", a);
        }
        else if(i==1){
            printf("%d ", b);
        }
        else{
            sum = a+b;
            printf("%d ", sum);
            a=b;
            b=sum;
        }
        
    }
           
    return 0;
}
