#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int size, i, ele, count=0;
    scanf("%d", &size);
    int arr[size];
    for(i=0; i<size; i++){
        scanf("%d", &arr[i]);
    }
    scanf("%d", &ele);
    for(i=0; i<size; i++){
        if(arr[i]==ele)
            count++;
    }
    printf("%d", count);
    return 0;
}
