#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int N, lastDigit, firstDigit;
    scanf("%d", &N);
    lastDigit = N % 10;
    firstDigit = N;

    while (firstDigit >= 10) {
        firstDigit /= 10;
    }

    printf("%d %d\n", firstDigit, lastDigit);
    return 0;
}
