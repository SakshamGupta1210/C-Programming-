#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, rev, rem=0, n1;
    scanf("%d", &n);
    n1 = n;
    
    while(n != 0){
        rem = n % 10;
        rev = rev * 10 + rem;
        n = n / 10;
    }
    if(n1 == rev)
        printf("Palindrome");
    else
        printf("Not Palindrome");
    return 0;
}
